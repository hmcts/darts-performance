    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Engine",
            "request": "launch",
            "mainClass": "Engine",
            "projectName": "darts-performance"
        },
        {
            "type": "java",
            "name": "Recorder",
            "request": "launch",
            "mainClass": "Recorder",
            "projectName": "darts-performance"
        },
        {
            "type": "java",
            "name": "Feeders",
            "request": "launch",
            "mainClass": "simulations.Scripts.Utilities.Feeders",
            "projectName": "darts-performance"
        },
        {
            "type": "java",
            "name": "RandomDateGenerator",
            "request": "launch",
            "mainClass": "simulations.Scripts.Utilities.RandomDateGenerator",
            "projectName": "darts-performance"
        },
        {
            "type": "java",
            "name": "TimestampGenerator",
            "request": "launch",
            "mainClass": "simulations.Scripts.Utilities.TimestampGenerator",
            "projectName": "darts-performance"
        }
    ]
}